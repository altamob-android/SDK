<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.altamob.android.sdkdemo">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NativedAdActivity"
            android:label="@string/natived_ad"/>

        <meta-data
            android:name="appKey"
            android:value="@string/app_key" />

        <activity android:name=".MessageFlowAdActivity"
            android:label="@string/data_flow_ad"/>
        <activity android:name=".AppWallActivity"
            android:label="@string/ad_wall"></activity>
    </application>

</manifest>